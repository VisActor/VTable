{
  "name": "@visactor/vtable-plugins",
  "version": "1.20.1",
  "description": "The search util of VTable",
  "author": {
    "name": "VisActor",
    "url": "https://VisActor.io/"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "cjs",
    "es",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./es/index.js"
    },
    "./gantt-extensions": {
      "require": "./cjs/gantt-export-image.js",
      "import": "./es/gantt-export-image.js"
    }
  },
  "scripts": {
    "demo": "vite ./demo",
    "compile": "tsc --noEmit",
    "build": "npm run fix-memory-limit && bundle --clean",
    "fix-memory-limit": "cross-env LIMIT=10240 increase-memory-limit"
  },
  "unpkg": "latest",
  "unpkgFiles": [
    "dist/vtable-plugins.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@visactor/vutils": "~0.19.1",
    "lodash": "4.17.21",
    "big.js": "6.2.2",
    "exceljs": "4.4.0",
    "file-saver": "2.0.5",
    "@types/file-saver": "2.0.7"
  },
  "peerDependencies": {
    "@visactor/vtable": "workspace:*",
    "@visactor/vtable-gantt": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@visactor/vtable-gantt": {
      "optional": true
    }
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "increase-memory-limit": "^1.0.7",
    "@visactor/vtable": "workspace:*",
    "@visactor/vtable-editors": "workspace:*",
    "@visactor/vtable-gantt": "workspace:*",
    "@visactor/vchart": "2.0.2",
    "@internal/bundler": "workspace:*",
    "@internal/eslint-config": "workspace:*",
    "@internal/ts-config": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "3.1.0",
    "eslint": "~8.18.0",
    "vite": "3.2.6",
    "typescript": "4.9.5",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@types/chai": "4.2.22",
    "@types/jest": "^26.0.0",
    "@types/mocha": "9.0.0",
    "@types/node": "*",
    "@types/offscreencanvas": "2019.6.4",
    "chai": "4.3.4",
    "jest": "^26.0.0",
    "jest-electron": "^0.1.12",
    "jest-transform-stub": "^2.0.0",
    "magic-string": "^0.25.7",
    "mocha": "9.1.3",
    "postcss": "8.4.21",
    "rimraf": "3.0.2",
    "sass": "1.43.5",
    "ts-jest": "^26.0.0",
    "ts-loader": "9.2.6",
    "ts-node": "10.9.0",
    "tslib": "2.3.1",
    "ttypescript": "1.5.13",
    "typescript-transform-paths": "3.3.1",
    "json-formatter-js": "^2.3.4",
    "inversify": "6.0.1",
    "vite-plugin-markdown": "^2.1.0",
    "markdown-it": "^13.0.0",
    "node-fetch": "2.6.7",
    "form-data": "~4.0.0",
    "axios": "^1.4.0",
    "@types/react-is": "^17.0.3",
    "rollup-plugin-node-resolve": "5.2.0",
    "@types/lodash": "4.14.182"
