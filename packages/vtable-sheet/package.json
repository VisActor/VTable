{
  "name": "@visactor/vtable-sheet",
  "version": "1.22.12-alpha.4",
  "description": "Lightweight editable spreadsheet component based on VTable",
  "keywords": [
    "vtable-sheet",
    "grid",
    "table",
    "spreadsheet",
    "sheet",
    "excel",
    "visualization",
    "VTable",
    "VisActor",
    "canvas",
    "datagrid",
